<h1>Shopping List</h1>
<form>
  <input type="text" ng-model='newSymbol' placeholder='Enter a symbol'>
  <input type="submit" value="Add">
</form>
<div ng-controller="ItemsController as ItemsCtrl">
  <ul>
    <li ng-repeat="item in ItemsCtrl.items">
      {{item.name}}, {{item.quantity}}
    </li>
  </ul>
</div>
